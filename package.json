{
  "name": "lunchclub",
  "version": "0.0.0",
  "description": "Lunch Club",
  "main": "dist/index.js",
  "scripts": {
    "dev": "npm run css && budo ./src/index.ts --live --pushstate --host=localhost --port=8888 --base=/ --serve=index.js -- -t sheetify/transform -t [ babelify ]  -p [ tsify ]",
    "css": "crux -v ./src/styles/vars.json -o ./src/styles/styles.css",
    "dist": "tsc && npm run browserify && npm run minify && npm run assets",
    "assets": "cp -R assets lib && cp index.html lib/index.html",
    "browserify": "browserify src/index.ts -p [ tsify ] -g babelify -g uglifyify -o lib/index.js",
    "minify": "uglifyjs ./lib/index.js --compress --mangle -o ./lib/index.js"
  },
  "dependencies": {
    "@types/node": "^7.0.5",
    "babel": "^6.23.0",
    "babel-cli": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "budo": "^9.4.7",
    "helix-react": "git://github.com/josephluck/helix-react",
    "moltin": "^1.0.5",
    "tsify": "^3.0.1",
    "tslint": "^4.4.2",
    "tslint-config-standard": "^4.0.0",
    "typescript": "^2.2.1",
    "uglifyify": "^3.0.4",
    "uglifyjs": "^2.4.10"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "crux-css": "2.0.4",
    "sheetify": "^6.0.1",
    "sheetify-postcss": "^1.1.0"
  }
}
